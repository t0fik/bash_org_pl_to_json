version: '3.7'
services:
  jenkins:
    image: {{jenkins.image}}
    privileged: true
    user: root
    ports:
      - 8080:8080
      - 50000:50000
    container_name: jenkins
    volumes:
      - {{jenkins.home}}:/var/jenkins_home
      - /var/run/docker.sock:/var/run/docker.sock
      - {{compose.docker_binary}}:/usr/local/bin/docker
